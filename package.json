{
    "name": "arbot-scraper",
    "version": "1.0.0",
    "author": "Kaltzisp",
    "description": "Arbot API scraper.",
    "main": "src/index.ts",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "build": "rm -rf app/* && npx tsc -p tsconfig.json",
        "cpfs": "cp ./package.json ./package-lock.json ./app && npm install --prefix=./app --omit=dev && rm ./app/node_modules/arbot-scraper && rm -r ./app/Models",
        "zip": "7z a -tzip arbscraper.zip ./app/* && mv arbscraper.zip ./app/arbscraper.zip",
        "deploy": "npm run build && npm run cpfs && npm run zip && aws lambda update-function-code --function-name=arbot-webscraper --zip-file fileb://app/arbscraper.zip"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "typescript": "latest"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "latest",
        "dotenv": "latest"
    }
}